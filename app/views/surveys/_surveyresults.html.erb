<% survey.questions.each do |question| %>
  <h3 class="title"><%= question.name %></h3>
  <% question.possible_values_to_array.each do |choice|%>
    <li><div class="choice"><%= choice %></div><div class="percentage"><%= question.percentage(choice)%> people chose this answer</div></li>
  <% end %>
  </ol>
<% end %>

